<%- include("header"); -%>

<body>

  <div class="box" id="heading">
    <h1><%=data%></h1>
  </div>


  <div class="box">


    <%  var f=element.length;%>
    <%for(var i=0;i<f;i++){%>
    <div class="item">
      <input type="checkbox">
      <p><%=element[i]%></p>
    </div>

    <%  }%>
    <form class="item" action="/" method="post">
      <!-- autocomplete="off"  is for to avoid autocomplete -->
      <input type="text" name="box" placeholder="New Item" autocomplete="off">
      <button type="submit" name="submit" value="<%=data%>">+</button>
    </form>
  </div>

</body>
<%- include('footer'); -%>
